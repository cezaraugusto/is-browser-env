{
  "name": "is-browser-env",
  "version": "1.0.0",
  "description": "Checks whether or not the current runtime environment is a browser",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "lint": "standard --verbose | snazzy",
    "prepare": "npm run build",
    "prepublishOnly": "npm audit",
    "test": "jest --config=jest.config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cezaraugusto/is-browser-env.git"
  },
  "keywords": [
    "is-browser",
    "is-nodejs",
    "browser",
    "environment",
    "conditional",
    "check"
  ],
  "bugs": {
    "url": "https://github.com/cezaraugusto/is-browser-env.git/issues"
  },
  "homepage": "https://github.com/cezaraugusto/is-browser-env.git#readme",
  "author": "Cezar Augusto <boss@cezaraugusto.net>",
  "license": "MIT",
  "standard": {
    "parser": "typescript-eslint-parser",
    "plugins": [
      "typescript",
      "markdown"
    ]
  },
  "devDependencies": {
    "@types/jest": "^23.3.11",
    "@types/node": "^10.12.18",
    "eslint-plugin-markdown": "^1.0.0-rc.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^23.6.0",
    "snazzy": "^8.0.0",
    "standard": "^14.3.1",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2",
    "typescript-eslint-parser": "^21.0.2"
  },
  "peerDependencies": {
    "eslint": "^5.11.1"
  }
}
